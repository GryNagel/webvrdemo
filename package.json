{
  "name": "webvrdemo",
  "description": "A-frame WebVR demo",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Gry Nagel",
  "scripts": {
    "start": "budo --live --verbose --port 3000 --open",
    "deploy": "ghpages",
